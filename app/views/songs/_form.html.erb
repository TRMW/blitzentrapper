<%= form_for @song do |f| %>
  <%= render 'shared/error_messages', :object => @song %>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :player %><br />
    <%= f.text_area :player %>
  </p>
  <p>
    <%= f.label :lyrics %><br />
    <%= f.text_area :lyrics %>
  </p>
  <p><%= f.submit %></p>
<% end %>
