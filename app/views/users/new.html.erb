<% title "Register" %>

<h1>Register</h1>

<%= form_for @user, :url => users_path, :html => { :multipart => true } do |f| %>
  <%= render 'shared/error_messages', :object => @user %>
  <%= render :partial => "form", :object => f %>
  <%= recaptcha_tags %>
  <%= f.submit "Register", :class => "button" %>
<% end %>
