<% if !@show.songs.blank? %>
	$("#setlist_link").html("<%= escape_javascript(link_to 'edit', edit_setlist_path(@show.id), :remote => true, :'data-type' => 'script') %>");
	$("#tracks").html("<%= escape_javascript(render :partial => 'display_setlist', :object => @show) %>");
<% else %>
	$("#setlist_link").html("<%= escape_javascript(link_to 'add', edit_setlist_path(@show.id), :remote => true, :'data-type' => 'script') %>");
	$("#tracks").html("");
<% end %>
