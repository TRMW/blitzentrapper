<% if !@show.songs.blank? %>
	$("#setlist_link").html("<%= escape_javascript(link_to "edit", url_for(:action => "edit_setlist", :id => @show.id), :remote => true) %>");
	$("#tracks").html("<%= escape_javascript(render :partial => 'display_setlist', :object => @show) %>");
<% else %>
	$("#setlist_link").html("<%= escape_javascript(link_to_remote "add", :url => { :action => "edit_setlist", :id => @show.id }) %>");
	$("#tracks").html("");
<% end %>